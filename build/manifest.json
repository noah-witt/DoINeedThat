{
    "manifest_version": 2,
    "name": "Do I Need That",
    "version": "0.0.2",
    "content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": ["jquery.js", "script/content.js"]
        }
      ],
    "background": {
      "scripts": ["jquery.js", "script/bg.js"]
    },
    "browser_action": {
      "default_title": "Do I Need That",
      "default_popup": "pages/popup.html"
    },
    "icons": { "16": "img/16.png",
      "48": "img/48.png",
     "128": "img/128.png" }
  }